[
  {
    "tenantId": "{tenantid}",
    "code": "CRS-ADMIN-CONSOLE.adminSchema",
    "description": "Boundary Management Schema for managing boundary data with latitude, longitude, and boundary codes",
    "isActive": true,
    "definition": {
      "type": "object",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "required": [
        "CRS_BOUNDARY_CODE",
        "CRS_LAT",
        "CRS_LONG"
      ],
      "x-unique": [
        "CRS_BOUNDARY_CODE"
      ],
      "properties": {
        "CRS_BOUNDARY_CODE": {
          "type": "string",
          "description": "Boundary Code",
          "minLength": 1,
          "maxLength": 200
        },
        "CRS_LAT": {
          "type": "number",
          "description": "Latitude",
          "minimum": -90,
          "maximum": 90
        },
        "CRS_LONG": {
          "type": "number",
          "description": "Longitude",
          "minimum": -180,
          "maximum": 180
        }
      },
      "additionalProperties": false
    }
  }
]
